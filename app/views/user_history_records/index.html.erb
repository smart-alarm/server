<h1>Listing user_history_records</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_history_records.each do |user_history_record| %>
      <tr>
        <td><%= user_history_record.arrival %></td>
        <td><%= user_history_record.on_time %></td>
      </tr>
    <% end %>
  </tbody>
</table>